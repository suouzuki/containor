{
  "name": "@suouzuki/containor",
  "version": "6.1.4",
  "description": "An enhanced Map-based data structure for Node.js, providing advanced collection management, iteration, and transformation utilities.",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.cjs"
    }
  },
  "main": "./dist/cjs/index.cjs",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "files": [
    "dist/",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build:esm": "tsc --project tsconfig.esm.json",
    "build:cjs": "tsc --project tsconfig.cjs.json",
    "build": "npm run build:esm && npm run build:cjs"
  },
  "keywords": [
    "containor",
    "map",
    "collection",
    "data-structure",
    "iterator",
    "utility",
    "enhanced-map",
    "key-value",
    "javascript",
    "nodejs",
    "esm",
    "commonjs",
    "dual-module"
  ],
  "author": {
    "name": "suouzuki",
    "url": "https://github.com/suouzuki"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/suouzuki/containor.git"
  },
  "bugs": {
    "url": "https://github.com/suouzuki/containor/issues"
  },
  "homepage": "https://github.com/suouzuki/containor#readme",
  "devDependencies": {
    "@types/node": "^24.10.0",
    "typescript": "^5.9.3"
  }
}
